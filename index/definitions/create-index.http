    "description": "",
    "dataSourceName": "{{DATASOURCE_NAME}}",
    "skillsetName": "{{SKILLSET_NAME}}",
    "targetIndexName": "{{DOCS_INDEX_NAME}}",
    "disabled": null,
    "schedule": {},
    "parameters": {
        "batchSize": null,
        "maxFailedItems": 10,
        "maxFailedItemsPerBatch": 5,
