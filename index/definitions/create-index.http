            "dataToExtract": "contentAndMetadata",
            "parsingMode": "default",
            "imageAction": "generateNormalizedImages"
        }
    },
    "fieldMappings": [],
    "outputFieldMappings": [],
    "cache": null,
    "encryptionKey": null
}

